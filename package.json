{
  "name": "auto-route-finder",
  "displayName": "Auto Route Finder",
  "description": "Helps Flutter developers easily jump to the definition of classes generated by the AutoRoute package",
  "license": "MIT",
  "publisher": "CreevekCZ",
  "author": {
    "name": "Jan Kožnárek",
    "email": "jan@koznarek.cz",
    "url": "https://jan.koznarek.cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CreevekCZ/auto-route-finder"
  },
  "icon": "assets/icon.png",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:dart"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "auto-route-finder.jumpToDefinition",
        "title": "Jump to Definition",
        "category": "Auto Route"
      },
      {
        "command": "auto-route-finder.refresh",
        "title": "Refresh Route Definitions",
        "category": "Auto Route"
      }
    ],
    "languages": [
      {
        "id": "dart"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^2.32.0",
    "eslint": "^9.23.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.2",
    "undici": "^6.19.0",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  }
}
